{
  "name": "phoenix",
  "private": true,
  "version": "0.0.1",
  "description": "Rising from the ashes of its predecessor",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "capacitor:sync": "npm run build && npx cap sync",
    "capacitor:copy": "npm run build && npx cap copy",
    "capacitor:sync:ios": "npm run build && npx cap sync ios",
    "capacitor:run:ios": "npx cap run ios",
    "capacitor:open:ios": "npx cap open ios",
    "capacitor:sync:android": "npm run build && npx cap sync android",
    "capacitor:open:android": "npx cap open android"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.7.0",
    "@stencil/core": "3.0.0",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "puppeteer": "^19.7.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stencil-router-v2": "^0.6.0",
    "typescript": "^4.9.5"
  },
  "license": "MIT",
  "dependencies": {
    "@capacitor-community/file-opener": "^1.0.4",
    "@capacitor-community/sqlite": "^4.6.3-1",
    "@capacitor/android": "^4.7.0",
    "@capacitor/core": "^4.7.0",
    "@capacitor/filesystem": "^4.1.4",
    "@capacitor/ios": "^4.7.0",
    "@stencil/store": "^2.0.4",
    "file-saver": "^2.0.5",
    "jeep-sqlite": "^1.7.1",
    "jszip": "^3.10.1"
  }
}
